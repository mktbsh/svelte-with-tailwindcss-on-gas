{
  "name": "svelte-with-tailwindcss-on-gas",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint:prettier": "prettier --check --ignore-path .gitignore \"!package-lock.json\" --plugin-search-dir=. ./**/*.{json,css,js,ts,cjs,svelte}",
    "fix:prettier": "prettier --write --ignore-path .gitignore \"!package-lock.json\" --plugin-search-dir=. ./**/*.{json,css,js,ts,cjs,svelte}",
    "lint:eslint": "eslint --ignore-path .gitignore . --ext .js,.ts,.svelte",
    "fix:eslint": "eslint --fix --ignore-path .gitignore . --ext .js,.ts,.svelte",
    "lint:stylelint": "stylelint --ignore-path .gitignore ./**/*.{css,svelte}",
    "fix:stylelint": "stylelint --fix --ignore-path .gitignore ./**/*.{css,svelte}",
    "lint:all": "run-s lint:eslint lint:stylelint lint:prettier",
    "fix:all": "run-s fix:eslint fix:stylelint fix:prettier",
    "copy:example": "cpx -C 'configs/example/*' dist/example && cpx 'configs/example/.clasp.json' dist/example && cpx 'src/server/*' dist/example && cpx 'dist/index.html' dist/example",
    "clean:example": "rimraf ./dist/example",
    "deploy:example": "cd dist/example && clasp push && clasp deploy && cd ../..",
    "release:example": "run-s build copy:example deploy:example clean:example"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@tsconfig/svelte": "^3.0.0",
    "@types/google-apps-script": "^1.0.51",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "autoprefixer": "^10.4.7",
    "cpx": "^1.5.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "rimraf": "^3.0.2",
    "stylelint": "^14.9.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^26.0.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vite-plugin-singlefile": "^0.10.0"
  },
  "dependencies": {
    "gas-client": "^1.0.0"
  }
}
